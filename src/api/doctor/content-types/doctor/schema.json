{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "fee": {
      "type": "integer",
      "required": true
    },
    "rating": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "experience": {
      "type": "integer",
      "required": true
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctor_id"
    },
    "speciality_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::speciality.speciality",
      "inversedBy": "doctors"
    },
    "Doctor_TimeSlots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::doctor-time-slot.doctor-time-slot",
      "mappedBy": "doctor_id"
    }
  }
}
