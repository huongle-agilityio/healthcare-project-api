{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "fee": {
      "type": "integer",
      "required": true
    },
    "rating": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "experience": {
      "type": "integer",
      "required": true
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctorId"
    },
    "specialty": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::specialty.specialty",
      "inversedBy": "doctors"
    },
    "bookingTimeSlots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking-time-slot.booking-time-slot",
      "mappedBy": "doctor"
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
